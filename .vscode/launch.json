{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Parse Test File",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/bin/antlr-groovy.ts",
            "args": [
                "parseTree",
                "test/test.groovy"
            ],
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "console": "internalConsole",
            "outputCapture": "std"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Parse Current File for functions",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/bin/antlr-groovy.ts",
            "args": [
                "findMethods",
                "${workspaceFolder}/test/debug.groovy"
            ],
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "console": "internalConsole",
            "outputCapture": "std"
        }
    ]
}